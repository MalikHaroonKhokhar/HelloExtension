{
    "manifest_version": 3,
    "name": "Image Replacer",
    "version": "1.0",
    "description": "Replace all images with 'Hello'",
    "permissions": ["scripting", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.jpg",
        "48": "icons/icon48.jpg",
        "128": "icons/icon128.jpg"
      }
    },
    "icons": {
      "16": "icons/icon16.jpg",
      "48": "icons/icon48.jpg",
      "128": "icons/icon128.jpg"
    }
  }
  